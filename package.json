{
  "name": "@telanflow/node-pty-prebuilt",
  "description": "Fork pseudoterminals in Node.JS",
  "author": {
    "name": "Microsoft Corporation"
  },
  "version": "0.9.1",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./typings/node-pty.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/telanflow/node-pty-prebuilt.git"
  },
  "files": [
    "binding.gyp",
    "lib/",
    "scripts/",
    "src/",
    "deps/",
    "typings/"
  ],
  "homepage": "https://github.com/telanflow/node-pty-prebuilt",
  "bugs": {
    "url": "https://github.com/telanflow/node-pty-prebuilt/issues"
  },
