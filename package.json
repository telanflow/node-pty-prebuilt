  "dependencies": {
    "nan": "^2.15.0",
    "prebuild-install": "^6.1.4"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "14.17.32",
    "cross-env": "^7.0.3",
    "mocha": "^9.1.3",
    "prebuild": "^11.0.0",
    "ps-list": "^6.0.0",
    "tslint": "^5.12.1",
